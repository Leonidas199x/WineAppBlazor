    <link href="_content/MudBlazor/MudBlazor.min.css" rel="stylesheet" />
</head>
<body>
    <script type='text/javascript' src='https://www.bing.com/api/maps/mapcontrol?callback=GetMap&key=AlawPhAIwg7QQ9H4x-OG6Pwak1wMv_frLYhQVqgj6HQ9V9RTmEEHiVlTbz429Fg3' async defer></script>
    <script src="js/ThemeChanger.js"></script>
    <script src="js/BingMaps.js"></script>
    <component type="typeof(App)" render-mode="ServerPrerendered" />

    <div id="blazor-error-ui">
